<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import org.controlsfx.control.textfield.CustomTextField?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<AnchorPane fx:id="mainSearchPane" minHeight="-Infinity" minWidth="-Infinity" prefHeight="80.0" prefWidth="600.0" stylesheets="@Dialogs.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.photoslide.search.SearchToolsController">
   <children>
      <Button id="toolbutton" fx:id="closeAction" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" text="Button" AnchorPane.leftAnchor="-20.0" AnchorPane.topAnchor="-10.0">
         <graphic>
            <FontIcon iconLiteral="ti-close" />
         </graphic>
      </Button>
      <VBox layoutX="21.0" layoutY="19.875" prefHeight="70.0" prefWidth="511.0" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="25.0">
         <children>
            <HBox alignment="CENTER_LEFT" minHeight="45.0" prefHeight="45.0" spacing="5.0" VBox.vgrow="NEVER">
               <children>
                  <FontIcon fx:id="sIcon" iconLiteral="ti-search" iconSize="40" />
                  <CustomTextField id="searchtext-field" fx:id="searchTextField" onKeyPressed="#searchTextFieldAction" prefHeight="50.0" prefWidth="382.0" promptText="Mediafile search" HBox.hgrow="ALWAYS">
                     <font>
                        <Font size="24.0" />
                     </font></CustomTextField>
               </children>
            </HBox>
            <VBox fx:id="searchResultVBox" prefHeight="300.0" prefWidth="100.0" VBox.vgrow="ALWAYS" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
